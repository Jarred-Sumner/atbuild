{
  "name": "atbuild-extension",
  "version": "0.0.1",
  "engines": {
    "vscode": ">=0.9.0-pre.1"
  },
  "publisher": "me",
  "contributes": {
    "languages": [
      {
        "id": "@js",
        "aliases": [
          "AtBuild Javascript",
          "@js"
        ],
        "extensions": [
          ".@js",
          ".atjs"
        ]
      }
    ],
    "grammars": [
      {
        "language": "@js",
        "scopeName": "source.@js",
        "path": "./syntaxes/@js.tmLanguage"
      }
    ]
  }
}

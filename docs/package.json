{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "prebuild": "yarn build-eval-worker; yarn copy-samples",
    "precopy-samples": "rm -rf samples",
    "copy-samples": "cp -r ../samples ./samples",
    "start": "next start",
    "build-eval-worker": "esbuild ./lib/EvalWorker.worker.ts --bundle --format=iife --outfile=./public/eval/EvalWorker.worker.js",
    "prebuild-eval-worker": "mkdir -p public/eval; cp node_modules/esbuild-wasm/esbuild.wasm ./public/eval/esbuild.wasm"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.19",
    "@next/mdx": "^10.0.1",
    "@rehooks/component-size": "^1.0.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-workers": "^1.0.0",
    "atbuild": "1.5.0",
    "esbuild": "^0.8.4",
    "esbuild-wasm": "^0.8.4",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "mdast-util-from-markdown": "^0.8.1",
    "mdast-util-toc": "^5.0.4",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "next": "10.0.1",
    "next-transpile-modules": "^4.1.0",
    "prettier": "^2.1.2",
    "prismjs": "^1.22.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-json-view": "^1.19.1",
    "react-monaco-editor": "^0.40.0",
    "react-simple-code-editor": "^0.11.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-parse": "^9.0.0",
    "remark-prism": "^1.1.17",
    "remark-slug": "^6.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "slugify": "^1.4.6",
    "unist-builder": "^2.0.3",
    "url-loader": "^4.1.1",
    "worker-loader": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.55",
    "typescript": "^4.0.5"
  }
}

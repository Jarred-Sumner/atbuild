@export function $multipleFields(fields) {
  @run {
    let result = 0;
  @}

  @build {
    for (let i = fields.length - 1; i >= 0; i--) {
      const field = fields[i];
      @run {
        result <<= @(schema[field]);
        result |= @(field);
      @}
    }
  @}

  @run {
    return result;
  @}

  @}